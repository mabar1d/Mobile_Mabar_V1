<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mabar_v1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mabar_V1">
        <activity android:name=".profile.DetailProfileAccountActivity"></activity>
        <activity android:name=".profile.HostSettingActivity" />
        <activity android:name=".profile.TeamSettingActivity" />
        <activity android:name=".splash.SplashScreen1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeActivity"></activity>
        <activity android:name=".signup.SignUpActivity" />
        <activity android:name=".login.LoginActivity" />
        <activity
            android:name=".MainActivity"
            android:layout_alignParentBottom="true"
            android:fitsSystemWindows="true"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustNothing"></activity>
    </application>

</manifest>